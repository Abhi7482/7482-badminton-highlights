{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["backend/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  